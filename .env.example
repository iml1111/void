# Application
ENV=development

# MongoDB
MONGODB_URI=mongodb://localhost:27017
MONGODB_NAME=void

# AWS Credentials
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_REGION=ap-northeast-2

# AWS SQS (FIFO Queue)
# Note: FIFO queue must have ContentBasedDeduplication=false
SQS_QUEUE_URL=https://sqs.ap-northeast-2.amazonaws.com/your-account/void-tasks.fifo

